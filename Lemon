function Lemon(_x, _y){
    this.x = _x;
    this.y = _y;

    this.xSpeed = 0;
    this.ySpeed = random(1, 2); // 1 - 2 (falling)
    this.rSpeed = random(-.02, .02); // rotation speed

    this.angle = 0;

    this.display = function(){
        push();
        translate(this.x, this.y);
        rotate(this.angle);
        
        pop();
    }
}